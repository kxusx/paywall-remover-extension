{
    "manifest_version": 3,
    "name": "URL Modifier",
    "version": "1.0",
    "description": "Modify the current tab's URL and open it in a new tab.",
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  